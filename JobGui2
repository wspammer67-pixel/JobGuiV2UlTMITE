-- StarterGui.JobScreenGui.JobGui.GoHome.LocalScript
local function C_e()
local script = G2L["e"];
	local player = game.Players.LocalPlayer
	local bb = script.Parent
	
	bb.MouseButton1Click:Connect(function()
		player:Kick("Go Home its a early day")
		print(player.name.."Left the game")
	end)
	
end;
task.spawn(C_e);
