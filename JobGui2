-- StarterGui.JobScreenGui.JobGui.GiveJob.LocalScript
local function C_8()
local script = G2L["8"];
	local player = game.Players.LocalPlayer
	local bb = script.Parent
	local JobGui = script.Parent.Parent
	local JobTool = game.ReplicatedStorage.Tool.Job
	
	bb.MouseButton1Click:Connect(function()
		JobTool:Clone().Parent = player.Backpack
		JobGui.Visible = true
		print(player.name.."Got a job")
	end)
	
end;
task.spawn(C_8);
-- StarterGui.JobScreenGui.JobGui.GiveJob.Hover
local function C_9()
local script = G2L["9"];
	local TL = script.Parent 
	TL.Active = true
	
	local TS = game:GetService("TweenService")
	
	local tweenInfo = TweenInfo.new(
	
		Enum.EasingStyle.Quad,
		Enum.EasingDirection.Out
	)
	
	local normalSize = TL.Size
	local hoverSize = UDim2.new(
		normalSize.X.Scale * 1.05,
		normalSize.X.Offset,
		normalSize.Y.Scale * 1.05,
		normalSize.Y.Offset
	)
	
	TL.MouseEnter:Connect(function()
		TS:Create(TL, tweenInfo, {
			Size = hoverSize
		}):Play()
	end)
	
	TL.MouseLeave:Connect(function()
		TS:Create(TL, tweenInfo, {
			Size = normalSize
		}):Play()
	end)
	
end;
task.spawn(C_9);
-- StarterGui.JobScreenGui.JobGui.FireFromJob.LocalScript
local function C_b()
local script = G2L["b"];
	local Players = game:GetService("Players")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	
	local player = Players.LocalPlayer
	local button = script.Parent  -- This should be the "FireFromJob" TextButton
	
	-- Change this if your tool has a different name inside ReplicatedStorage.Tools
	local JOB_TOOL_NAME = "Job"  
	
	button.MouseButton1Click:Connect(function()
		-- Get character's humanoid to unequip tools
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid:UnequipTools()  -- Moves any equipped tool back to Backpack
		end
	
		-- Remove the tool from Backpack
		local backpack = player:WaitForChild("Backpack")
		local toolInBackpack = backpack:FindFirstChild(JOB_TOOL_NAME)
		if toolInBackpack then
			toolInBackpack:Destroy()
			print(player.Name .. " has been fired")
		end
	
		-- Safety check: Remove from Character if somehow still there
		local toolInCharacter = character:FindFirstChild(JOB_TOOL_NAME)
		if toolInCharacter then
			toolInCharacter:Destroy()
			print(player.Name .. " has been fired")
		end
	
	end)
end;
task.spawn(C_b);
-- StarterGui.JobScreenGui.JobGui.FireFromJob.Hover
