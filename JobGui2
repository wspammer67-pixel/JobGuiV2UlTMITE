	TL.MouseEnter:Connect(function()
		TS:Create(TL, tweenInfo, {
			Size = hoverSize
		}):Play()
	end)
	
	TL.MouseLeave:Connect(function()
		TS:Create(TL, tweenInfo, {
			Size = normalSize
		}):Play()
	end)
	
end;
