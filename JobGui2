-- StarterGui.JobScreenGui.JobGui.Noclip.LocalScript
local function C_17()
local script = G2L["17"];
	-- Noclip GUI toggle LocalScript
	-- Place this as:
	-- 1) a LocalScript directly under your TextButton, OR
	-- 2) a LocalScript in StarterPlayerScripts (see findButton() comment)
	
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local playerGui = player:WaitForChild("PlayerGui")
	
	-- CONFIG: If your button is in a ScreenGui, set these names accordingly.
	-- If this script is parented to the TextButton, you don't need to change anything.
	local SCREEN_GUI_NAME = "NoclipGui"   -- name of ScreenGui (if using StarterPlayerScripts)
	local BUTTON_NAME = "NoclipButton"    -- name of TextButton
	
	-- If you placed this script directly under the button, this will return script.Parent
	local function findButton()
		if script.Parent:IsA("TextButton") or script.Parent:IsA("ImageButton") then
			return script.Parent
		end
	
		-- Otherwise try to find button under a named ScreenGui in PlayerGui
		local screen = playerGui:FindFirstChild(SCREEN_GUI_NAME)
		if screen then
			return screen:FindFirstChild(BUTTON_NAME, true) -- search descendants
		end
	
		-- fallback: look for any button named BUTTON_NAME anywhere in PlayerGui
		return playerGui:FindFirstChild(BUTTON_NAME, true)
	end
	
	local button = findButton()
	if not button then
		warn("Noclip script: Button not found. Put this LocalScript under your TextButton or set SCREEN_GUI_NAME/BUTTON_NAME correctly.")
		return
	end
	
	local noclip = false
	local character = player.Character or player.CharacterAdded:Wait()
	
	local function setCharacterCollision(chr, enabled)
		if not chr then return end
		for _, obj in pairs(chr:GetDescendants()) do
			if obj:IsA("BasePart") then
				obj.CanCollide = enabled
			end
		end
	end
	
	-- update GUI visuals
	local function updateButtonVisuals()
		if not button then return end
		if noclip then
			button.Text = "Noclip: ON"
			pcall(function() button.BackgroundColor3 = Color3.fromRGB(60, 170, 60) end)
		else
			button.Text = "Noclip: OFF"
			pcall(function() button.BackgroundColor3 = Color3.fromRGB(170, 60, 60) end)
		end
	end
	
	-- toggle function
	local function toggleNoclip()
		noclip = not noclip
		if noclip then
			setCharacterCollision(character, false)
		else
			setCharacterCollision(character, true)
		end
		updateButtonVisuals()
	end
	
	-- keep collisions off while noclip is active (handles new parts)
	RunService.Stepped:Connect(function()
		if noclip and character then
			for _, obj in pairs(character:GetDescendants()) do
				if obj:IsA("BasePart") then
					if obj.CanCollide then
						obj.CanCollide = false
					end
				end
			end
		end
	end)
	
	-- handle click
	button.MouseButton1Click:Connect(function()
		toggleNoclip()
	end)
	
	-- handle respawn
	local function onCharacterAdded(chr)
		character = chr
		if noclip then
			-- reapply noclip to the new character
			setCharacterCollision(character, false)
		else
			-- ensure normal collisions
			setCharacterCollision(character, true)
		end
	end
	
	player.CharacterAdded:Connect(onCharacterAdded)
	
	-- initial visuals & state
	if player.Character then
		onCharacterAdded(player.Character)
	end
	updateButtonVisuals()
	
end;
task.spawn(C_17);
-- StarterGui.JobScreenGui.JobGui.JumpBoost.LocalScript
local function C_1b()
local script = G2L["1b"];
	-- Place this LocalScript under a TextButton in your GUI
	
	local button = script.Parent
	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	local jumpBoostEnabled = false
	local NORMAL_JUMP = 50
	local BOOSTED_JUMP = 120
	
	-- Function to update visuals
	local function updateButtonVisuals()
		if jumpBoostEnabled then
			button.Text = "Jump Boost: ON"
			button.BackgroundColor3 = Color3.fromRGB(60, 170, 60)
		else
			button.Text = "Jump Boost: OFF"
			button.BackgroundColor3 = Color3.fromRGB(170, 60, 60)
		end
	end
	
	-- Function to toggle jump power
	local function toggleJumpBoost()
		local character = player.Character or player.CharacterAdded:Wait()
		local humanoid = character:FindFirstChildOfClass("Humanoid")
		if not humanoid then return end
	
		if jumpBoostEnabled then
			humanoid.JumpPower = NORMAL_JUMP
		else
			humanoid.JumpPower = BOOSTED_JUMP
		end
	
		jumpBoostEnabled = not jumpBoostEnabled
		updateButtonVisuals()
	end
	
	-- Connect button click
	button.MouseButton1Click:Connect(toggleJumpBoost)
	
	-- Handle respawn to reapply jump power
	player.CharacterAdded:Connect(function(character)
		local humanoid = character:WaitForChild("Humanoid")
		if jumpBoostEnabled then
			humanoid.JumpPower = BOOSTED_JUMP
		else
			humanoid.JumpPower = NORMAL_JUMP
		end
	end)
	
	-- Initialize
	updateButtonVisuals()
	
end;
task.spawn(C_1b);
-- StarterGui.JobScreenGui.JobGui.Fly.LocalScript
local function C_1f()
local script = G2L["1f"];
	-- Fly GUI toggle LocalScript
	-- Place this as:
	-- 1) a LocalScript directly under your TextButton, OR
	-- 2) a LocalScript in StarterPlayerScripts (adjust findButton names below)
	
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")
	local UserInputService = game:GetService("UserInputService")
	
	local player = Players.LocalPlayer
	local playerGui = player:WaitForChild("PlayerGui")
	
	-- CONFIG: change these if needed
	local SCREEN_GUI_NAME = "FlyGui"    -- name of ScreenGui (if using StarterPlayerScripts)
	local BUTTON_NAME = "FlyButton"     -- name of TextButton
	local FLY_SPEED = 100               -- horizontal speed
	local ASCEND_SPEED = 80             -- vertical speed when holding Space
	local DESCEND_SPEED = 80            -- vertical speed when holding LeftShift
	local MAX_VELOCITY = 200            -- safety clamp
	
	-- find button similar to previous script logic
	local function findButton()
		if script.Parent:IsA("TextButton") or script.Parent:IsA("ImageButton") then
			return script.Parent
		end
		local screen = playerGui:FindFirstChild(SCREEN_GUI_NAME)
		if screen then
			-- search descendants for the button name
			return screen:FindFirstChild(BUTTON_NAME, true)
		end
		return playerGui:FindFirstChild(BUTTON_NAME, true)
	end
	
	local button = findButton()
	if not button then
		warn("Fly script: Button not found. Put this LocalScript under your TextButton or set SCREEN_GUI_NAME/BUTTON_NAME correctly.")
		return
	end
	
	-- state
	local flying = false
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:FindFirstChildOfClass("Humanoid")
	local hrp = character:FindFirstChild("HumanoidRootPart")
	
	-- utility: (re)obtain instances after respawn
	local function refreshReferences(chr)
		character = chr
		humanoid = character:FindFirstChildOfClass("Humanoid")
		hrp = character:FindFirstChild("HumanoidRootPart")
	end
	
	-- Body objects (created when we start flying)
	local bodyVelocity = nil
	local bodyGyro = nil
	
	local function createBodies()
		if not hrp then return end
		-- remove old if present
		if bodyVelocity and bodyVelocity.Parent then bodyVelocity:Destroy() end
		if bodyGyro and bodyGyro.Parent then bodyGyro:Destroy() end
	
		bodyVelocity = Instance.new("BodyVelocity")
		bodyVelocity.MaxForce = Vector3.new(1e5, 1e5, 1e5)
		bodyVelocity.Velocity = Vector3.new(0,0,0)
		bodyVelocity.P = 1250
		bodyVelocity.Parent = hrp
	
		bodyGyro = Instance.new("BodyGyro")
		bodyGyro.MaxTorque = Vector3.new(1e5, 1e5, 1e5)
		bodyGyro.D = 100
		bodyGyro.P = 3000
		bodyGyro.CFrame = hrp.CFrame
		bodyGyro.Parent = hrp
	end
	
	local function destroyBodies()
		if bodyVelocity then
			pcall(function() bodyVelocity:Destroy() end)
			bodyVelocity = nil
		end
		if bodyGyro then
			pcall(function() bodyGyro:Destroy() end)
			bodyGyro = nil
		end
	end
	
	local function updateButtonVisuals()
		if not button then return end
		if flying then
			button.Text = "Fly: ON"
			pcall(function() button.BackgroundColor3 = Color3.fromRGB(60,170,60) end)
		else
			button.Text = "Fly: OFF"
			pcall(function() button.BackgroundColor3 = Color3.fromRGB(170,60,60) end)
		end
	end
	
	-- toggle fly
	local function startFlying()
		if flying then return end
		if not character or not hrp or not humanoid then return end
		flying = true
		-- disable default physics/jumping clientside (prevents weird jumps)
		if humanoid then
			humanoid.PlatformStand = true
		end
		createBodies()
		updateButtonVisuals()
	end
	
	local function stopFlying()
		if not flying then return end
		flying = false
		if humanoid then
			-- restore normal controls
			humanoid.PlatformStand = false
		end
		destroyBodies()
		updateButtonVisuals()
	end
	
	local function toggleFly()
		if flying then
			stopFlying()
		else
			startFlying()
		end
	end
	
	-- input helpers for movement
	local function getMovementInput()
		local forward = 0
		local right = 0
		if UserInputService:IsKeyDown(Enum.KeyCode.W) then forward = forward + 1 end
		if UserInputService:IsKeyDown(Enum.KeyCode.S) then forward = forward - 1 end
		if UserInputService:IsKeyDown(Enum.KeyCode.A) then right = right - 1 end
		if UserInputService:IsKeyDown(Enum.KeyCode.D) then right = right + 1 end
		return Vector3.new(right, 0, forward)
	end
	
	-- main loop: apply velocity according to camera direction + input
	RunService.RenderStepped:Connect(function(dt)
		if not flying then return end
		if not hrp or not bodyVelocity or not bodyGyro then return end
	
		local cam = workspace.CurrentCamera
		local camCFrame = cam and cam.CFrame or hrp.CFrame
		-- orientations
		local forwardVec = Vector3.new(camCFrame.LookVector.X, 0, camCFrame.LookVector.Z).Unit
		if forwardVec ~= forwardVec then forwardVec = Vector3.new(0,0,-1) end -- handle NaN
		local rightVec = Vector3.new(camCFrame.RightVector.X, 0, camCFrame.RightVector.Z).Unit
		if rightVec ~= rightVec then rightVec = Vector3.new(1,0,0) end
	
		-- movement input
		local inputVec = getMovementInput() -- (x = right, z = forward)
		local moveDir = (rightVec * inputVec.X + forwardVec * inputVec.Z)
		if moveDir.Magnitude > 1 then moveDir = moveDir.Unit end
		local horizontalVel = moveDir * FLY_SPEED
	
		-- vertical input
		local verticalVel = 0
		if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
			verticalVel = ASCEND_SPEED
		elseif UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
			verticalVel = -DESCEND_SPEED
		else
			verticalVel = 0
		end
	
		-- combine and clamp
		local finalVel = Vector3.new(horizontalVel.X, verticalVel, horizontalVel.Z)
		if finalVel.Magnitude > MAX_VELOCITY then
			finalVel = finalVel.Unit * MAX_VELOCITY
		end
	
		-- apply
		bodyVelocity.Velocity = finalVel
	
		-- orient the character to look in camera direction (smooth)
		local targetCFrame = CFrame.new(hrp.Position, hrp.Position + Vector3.new(camCFrame.LookVector.X, 0, camCFrame.LookVector.Z))
		bodyGyro.CFrame = targetCFrame
	end)
	
	
	button.MouseButton1Click:Connect(function()
		toggleFly()
	end)
	
	-- handle respawn: reattach bodies if flying
	local function onCharacterAdded(chr)
		refreshReferences(chr)
		if flying then
			-- small delay to allow parts to exist
			task.defer(function()
				hrp = character:WaitForChild("HumanoidRootPart", 5)
				humanoid = character:FindFirstChildOfClass("Humanoid")
				createBodies()
				if humanoid then humanoid.PlatformStand = true end
			end)
		else
			-- ensure not leaving anything behind
			destroyBodies()
		end
	end
	
	player.CharacterAdded:Connect(onCharacterAdded)
	if player.Character then
		onCharacterAdded(player.Character)
	end
	
	-- cleanup on script disable/unload
	script.AncestryChanged:Connect(function()
		if not script:IsDescendantOf(game) then
			stopFlying()
		end
	end)
	
	-- initial visuals
	updateButtonVisuals()
	
end;
task.spawn(C_1f);
-- StarterGui.JobScreenGui.JobGuiButton.LocalScript
local function C_23()
local script = G2L["23"];
	local player = game.Players.LocalPlayer
	local button = script.Parent
	local JobGui = script.Parent.Parent.JobGui
	
	local TS = game:GetService("TweenService")
	local tweenInfo = TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
	
	button.MouseButton1Click:Connect(function()
		if JobGui.Visible then
			local t = TS:Create(JobGui, tweenInfo, {Size = UDim2.fromScale(0, 0)})
			t:Play()
			t.Completed:Wait()
			JobGui.Visible = false
		else
			JobGui.Size = UDim2.fromScale(0, 0)
			JobGui.Visible = true
			TS:Create(JobGui, tweenInfo, {Size = UDim2.fromScale(1, 1)}):Play()
		end
	end)
	
end;
task.spawn(C_23);
